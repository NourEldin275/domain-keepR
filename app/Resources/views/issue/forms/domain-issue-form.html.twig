{{ form_start(form) }}
    <div class="form-group">
        {{ form_label(form.ticketReference) }}
        {{ form_errors(form.ticketReference) }}
        {{ form_widget(form.ticketReference, {'attr': {'class': 'form-control'}}) }}
    </div>

    <div class="form-group">
        {{ form_label(form.dateCreated) }}
        {{ form_errors(form.dateCreated) }}
        {{ form_widget(form.dateCreated, {'attr': {'class': 'form-control'}}) }}
    </div>

    <div class="form-group">
        {{ form_label(form.status) }}
        {{ form_errors(form.status) }}
        {{ form_widget(form.status, {'attr': {'class': 'form-control'}}) }}
    </div>

    <div>
        {{ form_widget(form.save, {'attr': {'class': 'btn btn-success'}, 'label': 'Save Domain Issue'}) }}
    </div>

{{ form_end(form) }}